import tkinter as tk

def calcular():
    try:
        num1 = float(entry num1.get())
        num2 = float(entry num2.get())
        resultado = num1  + num2
        label resultado.config(text="Resultado:" + str (resultado))
    except ValueError:
        label resultado.config(text="Por favor, insira número válidos")

        root = tk.tk()
        root.title("Calculadora")

        label_num1 = tk.Label(root, text = "Número ' :")
